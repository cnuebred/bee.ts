{
    "name": "bee.ts",
    "version": "1.1.8",
    "description": "little node.js package to create simple front for backend api",
    "main": "build/index.js",
    "types": "build/index.d.ts",
    "files": [
        "build"
    ],
    "scripts": {
        "dev": "npm run hive",
        "hive": "nodemon --watch . --ext ts,json,bee,js --exec npx ts-node src/app.ts",
        "wss": "nodemon --watch wss.ts --ext ts,json,bee,js --exec npx ts-node ./wss.ts",
        "build": "npx tsc"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/cnuebred/bee.ts.git"
    },
    "keywords": [
        "front",
        "bee",
        "hive",
        "api",
        "html",
        "module",
        "node",
        "minimal"
    ],
    "author": "cnuebred",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/cnuebred/bee.ts/issues"
    },
    "homepage": "https://github.com/cnuebred/bee.ts#readme",
    "dependencies": {
        "axios": "^0.27.2",
        "crypto": "^1.0.1",
        "fs": "^0.0.1-security",
        "jsdom": "^19.0.0",
        "typescript": "^4.6.3"
    },
    "devDependencies": {
        "@types/node": "^17.0.25",
        "@typescript-eslint/eslint-plugin": "^5.27.1",
        "@typescript-eslint/parser": "^5.27.1",
        "eslint": "^8.17.0",
        "eslint-config-google": "^0.14.0"
    }
}
